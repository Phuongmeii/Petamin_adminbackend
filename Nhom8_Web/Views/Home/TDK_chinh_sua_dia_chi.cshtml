}



<!--====== App Content ======-->
<div class="app-content">


    <!--====== Section 1 ======-->
    <div class="u-s-p-y-60">


        <!--====== Section Content ======-->
        <div class="section__content">
            <div class="container">
                <div class="breadcrumb">
                    <div class="breadcrumb__wrap">
                        <ul class="breadcrumb__list">
                            <li class="has-separator">


                                <a href="@Url.Action("index", "Home")">Home</a>
                            </li>
                            <li class="is-marked">


                                <a href="@Url.Action("TDK_Bang_dieu_khien", "Home")">Tài khoản của tôi</a>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!--====== End - Section 1 ======-->
    <!--====== Section 2 ======-->
    <div class="u-s-p-b-60">


        <!--====== Section Content ======-->
        <div class="section__content">
            <div class="dash">
                <div class="container">
                    <div class="row">
                        <div class="col-lg-3 col-md-12">


                            <!--====== Dashboard Features ======-->
                            <div class="dash__box dash__box--bg-white dash__box--shadow u-s-m-b-30">
                                <div class="dash__pad-1">


                                    <span class="dash__text u-s-m-b-16">Chào cậu!</span>
                                    <ul class="dash__f-list">
                                        <li>


                                            <a class="dash-active" href="@Url.Action("TDK_Bang_dieu_khien", "Home")">Quản lí tài khoản</a>
                                        </li>
                                        <li>


                                            <a href="@Url.Action("TDK_thong_tin_cua_toi", "Home")">Thông tin cá nhân</a>
                                        </li>
                                        <li>


                                            <a href="@Url.Action("TDK_dia_chi_giao_hang_book", "Home")">Địa chỉ giao hàng</a>
                                        </li>
                                        <li>


                                            <a href="@Url.Action("TDK_theo_doi_don_hang", "Home")">Theo dõi đơn hàng</a>
                                        </li>
                                        <li>


                                            <a href="@Url.Action("TDK_don_hang_cua_toi", "Home")">Đơn hàng của bạn</a>
                                        </li>
                                        <li>


                                            <a href="@Url.Action("TDK_phuong_thuc_thanh_toan", "Home")">Lựa chọn phương thức thanh toán</a>
                                        </li>
                                        <li>


                                            <a href="@Url.Action("TDK_huy_don_hang", "Home")">Đơn trả & huỷ</a>
                                        </li>
                                    </ul>
                                </div>
                            </div>
                            <div class="dash__box dash__box--bg-white dash__box--shadow dash__box--w">
                                <div class="dash__pad-1">
                                    <ul class="dash__w-list">
                                        <li>
                                            <div class="dash__w-wrap">


                                                <span class="dash__w-icon dash__w-icon-style-1"><i class="fas fa-cart-arrow-down"></i></span>


                                                <span class="dash__w-text">4</span>


                                                <span class="dash__w-name">Đơn đã đặt</span>
                                            </div>
                                        </li>
                                        <li>
                                            <div class="dash__w-wrap">


                                                <span class="dash__w-icon dash__w-icon-style-2"><i class="fas fa-times"></i></span>


                                                <span class="dash__w-text">0</span>


                                                <span class="dash__w-name">Đơn huỷ</span>
                                            </div>
                                        </li>
                                        <li>
                                            <div class="dash__w-wrap">


                                                <span class="dash__w-icon dash__w-icon-style-3"><i class="far fa-heart"></i></span>


                                                <span class="dash__w-text">3</span>


                                                <span class="dash__w-name">Yêu thích</span>
                                            </div>
                                        </li>
                                    </ul>
                                </div>
                            </div>
                            <!--====== End - Dashboard Features ======-->
                        </div>
                        <div class="col-lg-9 col-md-12">
                            <div class="dash__box dash__box--shadow dash__box--radius dash__box--bg-white">
                                <div class="dash__pad-2">
                                    <h1 class="dash__h1 u-s-m-b-14">Chỉnh sửa địa chỉ</h1>


                                    <span class="dash__text u-s-m-b-30">Chúng tôi cần một địa chỉ để có thể giao hàng đến bạn.</span>
                                    <form class="dash-address-manipulation">
                                        <div class="gl-inline">
                                            <div class="u-s-m-b-30">


                                                <label class="gl-label" for="address-fname">Họ *</label>


                                                <input class="input-text input-text--primary-style" type="text" id="address-fname" placeholder="Nguyễn">
                                            </div>
                                            <div class="u-s-m-b-30">


                                                <label class="gl-label" for="address-lname">Tên *</label>


                                                <input class="input-text input-text--primary-style" type="text" id="address-lname" placeholder="Lan Anh">
                                            </div>
                                        </div>
                                        <div class="gl-inline">
                                            <div class="u-s-m-b-30">


                                                <label class="gl-label" for="address-phone">Số điện thoại *</label>


                                                <input class="input-text input-text--primary-style" type="text" id="address-phone" placeholder="(+0) 900901904">
                                            </div>
                                            <div class="u-s-m-b-30">


                                                <label class="gl-label" for="address-street">Địa chỉ *</label>


                                                <input class="input-text input-text--primary-style" type="text" id="address-street" placeholder="279 Nguyễn Tri Phương">
                                            </div>
                                        </div>
                                        <div class="gl-inline">
                                            <div class="u-s-m-b-30">


                                                <!--====== Select Box ======-->


                                                <label class="gl-label" for="address-country">Tỉnh *</label>
                                                <input class="input-text input-text--primary-style" type="text" id="address-country" placeholder="TP.HCM">
                                            </div>
                                            <!--====== End - Select Box ======-->
                                        </div>
                                        <div class="u-s-m-b-30">


                                            <!--====== Select Box ======-->


                                            <label class="gl-label" for="address-state">Khu vực *</label><select class="select-box select-box--primary-style" id="address-state">
                                                <option selected value="">Chọn khu vực</option>
                                                <option value="al">Nội địa</option>
                                                <option value="al">Ngoài nước</option>

                                            </select>
                                            <!--====== End - Select Box ======-->
                                        </div>
                                </div>
                                <div class="gl-inline">
                                    <div style="margin-left: 25px;" class="u-s-m-b-30">

                                        <label class="gl-label" for="address-city">Thành phố *</label>


                                        <input class="input-text input-text--primary-style" type="text" id="address-city">
                                    </div>
                                    <div class="u-s-m-b-30">


                                        <label class="gl-label" for="address-street">Mã zip </label>


                                        <input class="input-text input-text--primary-style" type="text" id="address-postal" placeholder="20006">
                                    </div>
                                </div>
                                <button style="margin-left: 25px;  font-size: 14px; height: 40px; width: 100px; border-radius: 5px;" class="btn btn--e-brand-b-2" type="submit">Lưu</button>

                                <br>
                                <br>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!--====== End - Section Content ======-->
</div>
<!--====== End - Section 2 ======-->
        </div>
